{
  "name": "northstar",
  "version": "1.0.0",
  "description": "Congressional Intelligence Platform - Full-blown enterprise product with AI-powered insights",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch backend/src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "tsx backend/src/server.ts",
    "lint": "eslint backend/src --ext .ts",
    "lint:fix": "eslint backend/src --ext .ts --fix",
    "format": "prettier --write \"backend/src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "sync": "tsx backend/src/scripts/sync.ts full",
    "sync:recent": "tsx backend/src/scripts/sync.ts recent",
    "sync:bills": "tsx backend/src/scripts/sync.ts bills",
    "sync:members": "tsx backend/src/scripts/sync.ts members",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "congress",
    "legislation",
    "bills",
    "api",
    "government",
    "civic-data",
    "congressional-intelligence",
    "ai",
    "analytics"
  ],
  "author": "Northstar",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.2.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "prisma": "^6.2.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
